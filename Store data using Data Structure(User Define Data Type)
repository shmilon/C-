#include <iostream>
using namespace std;

struct student{
    string name;
    int id;
    float cgpa;
    int age;
};
    
int main()
{   int n=3;
    student info[n];
    for(int i=0; i<n; i++){
        cin>>info[i].name;
        cin>>info[i].id;
        cin>>info[i].cgpa;
        cin>>info[i].age;
    }
    
    for(int i=0;i<n; i++){
        cout<<info[i].name;
        cout<<","<<info[i].id;
        cout<<","<<info[i].cgpa;
        cout<<","<<info[i].age;
        cout<<endl;
    }
    
}

